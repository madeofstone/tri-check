# Job Performance Monitor - Environment Configuration
# Copy this file to .env and fill in your values

# ===== Platform API Settings =====
# Base URL for the Alteryx Cloud Platform API
PLATFORM_API_BASE_URL=https://eu1.alteryxcloud.com/v4

# Your Platform API Bearer token
PLATFORM_API_TOKEN=your_platform_token_here

# Ranfor filter for job queries (comma-separated: recipe, plan)
RANFOR_FILTER=recipe,plan

# Default limit for API queries
DEFAULT_LIMIT=25

# Match window in minutes for pairing AAC â†” On-Prem jobs
MATCH_WINDOW_MINUTES=10

# ===== Databricks Settings =====
# Databricks workspace URL
DATABRICKS_HOST=https://your-workspace.azuredatabricks.net

# Databricks Personal Access Token (PAT)
DATABRICKS_TOKEN=your_databricks_pat_here

# ===== Job Naming =====
# Prefix used for Databricks job names (job ID will be appended)
DATABRICKS_JOB_NAME_PREFIX=AAC-Transform-Job-

# ===== Output Settings =====
# Default output file path
DEFAULT_OUTPUT_FILE=job_report.csv

# ===== On-Prem Trifacta API Settings =====
# Base URL for the on-prem Trifacta Platform API
ONPREM_API_BASE_URL=https://your-onprem-host/v4

# Your On-Prem API Bearer token
ONPREM_API_TOKEN=your_onprem_token_here
